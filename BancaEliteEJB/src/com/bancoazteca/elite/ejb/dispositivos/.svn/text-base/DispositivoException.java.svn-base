
package com.bancoazteca.elite.ejb.dispositivos;

/**
 * @author Banco Azteca S.A. [JFAV]
 * Agosto 25, 2008.
 */
public class DispositivoException extends Exception {

	private static final long serialVersionUID = -2470989720815108409L;


	/**
	 * @param message
	 */
	public DispositivoException(String message) {
		super(message);
	}

	/**
	 * @param cause
	 */
	public DispositivoException(Throwable cause) {
		super(cause);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public DispositivoException(String message, Throwable cause) {
		super(message, cause);
	}
}
