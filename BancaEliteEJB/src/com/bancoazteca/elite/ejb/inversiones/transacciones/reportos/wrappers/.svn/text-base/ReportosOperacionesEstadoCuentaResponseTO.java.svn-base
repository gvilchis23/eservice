package com.bancoazteca.elite.ejb.inversiones.transacciones.reportos.wrappers;


import java.util.Map;

import com.bancoazteca.elite.ejb.inversiones.transacciones.reportos.ReportosResponse;

public class ReportosOperacionesEstadoCuentaResponseTO extends ReportosResponse {

	public ReportosOperacionesEstadoCuentaResponseTO(Map<String, String> parametros) {
		super(parametros);
	}


	public String getFechaInicio() {
		return getAttribute( "PDRFI" );
	}


	public String getFechaVencimiento() {
		return getAttribute( "PDRFV" );
	}


	public String getPlazo() {
		return getAttribute( "PDRPZO" );
	}


	public String getTasaPremio() {
		return getAttribute( "PDRTP" );
	}


	public String getMontoInvertido() {
		return getAttribute( "PDRMI" );
	}


	public String getEmisora() {
		return getAttribute( "PDREMI" );
	}


	public String getSerie() {
		return getAttribute( "PDRSER" );
	}


	public String getTitulos() {
		return getAttribute( "PDRTIT" );
	}


	public String getPremioDevengado() {
		return getAttribute( "PDRPD" );
	}


	public String getValorCorte() {
		return getAttribute( "PDRVC" );
	}


	public String getTotalInvertido() {
		return getAttribute( "PDRTMI" );
	}


	public String getTotalValorCorte() {
		return getAttribute( "PDRTVC" );
	}

}
