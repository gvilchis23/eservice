
package com.bancoazteca.elite.ejb.pagoServicios;

/**
 * 
 * @author Jorge Bringas
 *
 */
public class PagoServiciosException extends Exception {


	private static final long serialVersionUID = -7521143031182466005L;


	public PagoServiciosException() { }


	public PagoServiciosException(String message) {
		super(message);
	}

	
	public PagoServiciosException(Throwable cause) {
		super(cause);
	}


	public PagoServiciosException(String message, Throwable cause) {
		super(message, cause);
	}

}
