package com.bancoazteca.elite.ejb.alnova.beans;

public class P027LocalizacionClientesRequestTO extends EjbAlnovaRequestTO {

	private static final long serialVersionUID = 1L;

	private static final String[] keys ={"COD1","COD2","CUSNM","SURNM","SFNM","BIRTHDA"};
		
		

	public P027LocalizacionClientesRequestTO() {
		super("P027", keys);
	}

	public String getCodigoIdentificacionUno() {
		return getAttribute("COD1");
	}

	public String getCodigoIdentificacionDos() {
		return getAttribute("COD2");
	}

	public String getNombreCliente() {
		return getAttribute("CUSNM");
	}

	public String getApellidoPaterno() {
		return getAttribute("SURNM");
	}

	public String getApellidoMaterno() {
		return getAttribute("SFNM");
	}

	public String getFechaNacimiento() {
		return getAttribute("BIRTHDA");
	}


	public void setCodigoIdentificacionUno(String value) {
		addParameter("COD1", value);
	}

	public void setCodigoIdentificacionDos(String value) {
		addParameter("COD2", value);
	}

	public void setNombreCliente(String value) {
		addParameter("CUSNM", value);
	}

	public void setApellidoPaterno(String value) {
		addParameter("SURNM", value);
	}

	public void setApellidoMaterno(String value) {
		addParameter("SFNM", value);
	}
	public void setFechaNacimiento(String value) {
		addParameter("BIRTHDA", value);
	}

	
}