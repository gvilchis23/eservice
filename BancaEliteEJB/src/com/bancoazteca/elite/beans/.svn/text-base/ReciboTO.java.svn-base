package com.bancoazteca.elite.beans;

import java.io.Serializable;
import java.util.Collection;
/**
 * @author Banco Azteca S.A. [JFAV] Diciembre 30, 2008.
 */
public class ReciboTO implements Serializable {

	/**
	 * Generated serialVersionUID
	 */
	private static final long serialVersionUID = 2361975786341513671L;

	//fields
	private ReciboCabeceraTO cabecera;
	private Collection<ReciboDetalleTO> percepcionDeduccions;
	private Collection<ReciboMensajeTO> mensajes;
	
	/**
	 * @return the cabecera
	 */
	public ReciboCabeceraTO getCabecera() {
		return cabecera;
	}
	/**
	 * @param cabecera the cabecera to set
	 */
	public void setCabecera(ReciboCabeceraTO cabecera) {
		this.cabecera = cabecera;
	}
	/**
	 * @return the percepcionDeduccions
	 */
	public Collection<ReciboDetalleTO> getPercepcionDeduccions() {
		return percepcionDeduccions;
	}
	/**
	 * @param percepcionDeduccions the percepcionDeduccions to set
	 */
	public void setPercepcionDeduccions(Collection<ReciboDetalleTO> percepcionDeduccions) {
		this.percepcionDeduccions = percepcionDeduccions;
	}
	/**
	 * @return the mensajes
	 */
	public Collection<ReciboMensajeTO> getMensajes() {
		return mensajes;
	}
	/**
	 * @param mensajes the mensajes to set
	 */
	public void setMensajes(Collection<ReciboMensajeTO> mensajes) {
		this.mensajes = mensajes;
	}

}
