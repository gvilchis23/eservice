package com.bancoazteca.elite.beans;

import java.io.Serializable;

public class EstadoCuentaRequestTO implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1499124981900825010L;

	private String user;
	
	private String cuenta;
	private String periodo;
	private String submit;
	private String cuentaFormateada;
	
	private String method;
	private String anio;
	private String mes;
	
	public void setCuenta(String cuenta) {
		this.cuenta = cuenta;
	}
	public String getCuenta() {
		return cuenta;
	}
	public void setPeriodo(String periodo) {
		this.periodo = periodo;
	}
	public String getPeriodo() {
		return periodo;
	}
	public void setSubmit(String submit) {
		this.submit = submit;
	}
	public String getSubmit() {
		return submit;
	}
	public void setCuentaFormateada(String cuentaFormateada) {
		this.cuentaFormateada = cuentaFormateada;
	}
	public String getCuentaFormateada() {
		return cuentaFormateada;
	}
	public void setUser(String user) {
		this.user = user;
	}
	public String getUser() {
		return user;
	}
	public String getMethod() {
		return method;
	}
	public void setMethod(String method) {
		this.method = method;
	}
	public String getAnio() {
		return anio;
	}
	public void setAnio(String anio) {
		this.anio = anio;
	}
	public String getMes() {
		return mes;
	}
	public void setMes(String mes) {
		this.mes = mes;
	}		
}